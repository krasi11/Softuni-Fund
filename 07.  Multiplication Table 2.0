using System;

namespace MultiplicationTable
{
    class Program
    {
        static void Main(string[] args)
        {
            int nummber = int.Parse(Console.ReadLine());
            int second = int.Parse(Console.ReadLine());
            if (second > 10)
            {
                Console.WriteLine($"{nummber} X {second} = {nummber * second}");
            }
            else
            {
                for (int i = second; i <= 10; i++)
                {
                    Console.WriteLine($"{nummber} X {i} = {nummber * i}");
                }
            }
        }
    }
}
